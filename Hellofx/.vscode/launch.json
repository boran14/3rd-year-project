    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "draughts.App",
            "projectName": "Hellofx"
        },
        {
            "type": "java",
            "name": "Launch GUI",
            "request": "launch",
            "vmArgs": "--module-path C:/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "mainClass": "draughts.GUI",
            "projectName": "Hellofx"
        },
        {
            "type": "java",
            "name": "Launch Runner",
            "request": "launch",
            "mainClass": "draughts.Runner",
            "projectName": "Hellofx"
        }
    ]
}